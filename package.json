    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-auto-catalog": "2.0.0-beta.203",
    "vuepress-plugin-copyright2": "2.0.0-beta.203",
